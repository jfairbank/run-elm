{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "plugins": [
    "import"
  ],
  "ignorePatterns": ["**/test/integration/case-projects/**/*.*", "test/js.js"],
  "rules": {
    "comma-dangle": 0,
    "global-require": 0,
    "no-console": 0,
    "prefer-destructuring": 0,
    "import/no-dynamic-require": 0
  }
}
